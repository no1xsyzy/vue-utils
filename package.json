{
  "name": "vue-utils",
  "description": "Useful tools for exercising Vue.js.",
  "version": "1.0.0",
  "author": "no1xsyzy <mf20070535@126.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "worktree": "git worktree add ../vue-utils-dist gh-pages",
    "build": "shx rm -rf ../vue-utils-dist/dist && shx cp index.html ../vue-utils-dist/index.html && cross-env NODE_ENV=production webpack --progress --hide-modules",
    "webpackinfo": "which webpack && webpack -v",
    "wdsinfo": "which webpack-dev-server && webpack-dev-server -v"
  },
  "dependencies": {
    "vue": "^2.3.3",
    "vue-router": "^2.5.3",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
